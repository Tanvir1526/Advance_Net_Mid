@model List<Lab3.Models.Database.Book>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="tabel table-bordered ">
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Description</th>
        <th>Category</th>
        <th>Publish Date</th>
    </tr>
    
    @foreach(var b in Model)
        {
        <tr>
            <td>b.Id</td>
            <td>b.Title</td>
            <td>b.Description</td>
            <td>b.Category</td>
            <td>b.PublishDate</td>
        </tr>

        }
    
</table>